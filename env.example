# CDP API Credentials (get from cdp.coinbase.com)
CDP_API_KEY_ID=organizations/your-org-id/apiKeys/your-key-id
CDP_API_KEY_SECRET="-----BEGIN EC PRIVATE KEY-----
your-private-key-here
-----END EC PRIVATE KEY-----"

# Base RPC endpoint for contract reads (e.g. https://mainnet.base.org)
BASE_RPC_URL=https://mainnet.base.org

# Subgraph endpoint used for MEME supply enforcement
MEME_SUBGRAPH_URL=https://api.goldsky.com/api/public/project_cm8plie9y1pjh01yea3kubv4c/subgraphs/MemeTokenV2-x402/0.0.1/gn

# IAO API Registry Subgraph URL (for querying registered APIs by fee destination address)
IAO_SUBGRAPH_URL=https://api.goldsky.com/api/public/project_cm8plie9y1pjh01yea3kubv4c/subgraphs/IAO-API-Registry/0.0.1/gn

# Receiving wallet for MEME payments
RECEIVER_WALLET_ADDRESS=0x9Fb6A270935E7B17a7D74682Bdd12b503C536e28

# Builder API Authentication
# Shared secret phrase for JWT authentication between proxy and builder endpoints
# This should be a strong, random string shared securely with builders
BUILDER_SECRET_PHRASE=your-secret-phrase-here

# Thirdweb x402 Facilitator Configuration
# Required for payment processing on /api/:address endpoints
# Get your secret key from: https://portal.thirdweb.com
# Go to Settings > API Keys > Create API Key
THIRDWEB_SECRET_KEY=your-thirdweb-secret-key-here

# Server wallet address for receiving payments
# This should be a wallet address you control (can be the same as RECEIVER_WALLET_ADDRESS)
# Get from your Thirdweb project dashboard or use your own wallet address
THIRDWEB_SERVER_WALLET_ADDRESS=0xYourServerWalletAddressHere

# DynamoDB Configuration
# For local development, set DYNAMODB_ENDPOINT=http://localhost:8000
DYNAMODB_REGION=us-east-1
DYNAMODB_TABLE_NAME=iao-tokens
USER_REQUEST_TABLE_NAME=iao-user-requests
REQUEST_QUEUE_TABLE_NAME=iao-request-queue
DYNAMODB_ENDPOINT=
